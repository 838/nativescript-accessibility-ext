{
  "name": "@nota/nativescript-accessibility-ext",
  "version": "3.0.0-alpha.5",
  "description": "Nativescript plugin for enabling accessiblity features",
  "main": "index.js",
  "nativescript": {
    "platforms": {
      "android": "3.0.0",
      "ios": "3.0.0"
    },
    "plugin": {
      "nan": true,
      "pan": true,
      "core3": true,
      "category": "Utilities"
    }
  },
  "scripts": {
    "sass": "rm -rf css && node-sass --recursive --output css scss",
    "build": "npm run tsc && npm run sass",
    "prepublish": "npm run build",
    "tsc": "tsc",
    "tsc-w": "tsc -w",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Notalib/nativescript-accessibility-ext.git"
  },
  "keywords": [
    "nativescript",
    "accessibility"
  ],
  "author": "Morten Sj√∏gren <m_abs@mabs.dk>",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/Notalib/nativescript-accessibility-ext/issues"
  },
  "homepage": "https://github.com/Notalib/nativescript-accessibility-ext/#readme",
  "devDependencies": {
    "nativescript-globalevents": "^1.1.0",
    "node-sass": "^4.5.3",
    "tns-core-modules": "^3.0.0",
    "tns-platform-declarations": "^3.0.0",
    "typescript": "^2.3.0"
  },
  "peerDependencies": {
    "tns-core-modules": "^3.0.0",
    "nativescript-globalevents": "^1.1.0"
  }
}
