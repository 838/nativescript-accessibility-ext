{
  "name": "@nota/nativescript-accessibility-ext",
  "version": "5.1.0-alpha.2",
  "description": "Add support for 'VoiceOver' and 'TalkBack' in NativeScript",
  "main": "index.js",
  "typings": "index.d.ts",
  "nativescript": {
    "platforms": {
      "android": "5.1.0",
      "ios": "5.1.0"
    },
    "plugin": {
      "nan": true,
      "pan": true,
      "core3": true,
      "category": "Utilities"
    }
  },
  "scripts": {
    "format": "prettier --write --config $(git rev-parse --show-toplevel)/.prettierrc.json '**/*.ts' '../demo-ng/app/**/*.ts' '../demo/app/**/*.ts'",
    "format:check": "prettier-check --config $(git rev-parse --show-toplevel)/.prettierrc.json '**/*.ts' '../demo-ng/app/**/*.ts' '../demo/app/**/*.ts'",
    "scss": "node-sass --recursive --output css scss",
    "tsc": "tsc -skipLibCheck",
    "build": "npm ci && npm-run-all lint tsc scss",
    "tslint": "tslint '**/*.ts' '../demo-ng/app/**/*.ts' '../demo/app/**/*.ts' --config $(git rev-parse --show-toplevel)/tslint.json --exclude \"**/node_modules/**\"",
    "tslint:fix": "npm run tslint -- --fix",
    "lint": "npm-run-all tslint format:check",
    "plugin.link": "npm link && npm-run-all plugin.link.demo-ng",
    "plugin.link.demo-ng": "cd ../demo-ng && npm link nativescript-accessibility-ext",
    "plugin.tscwatch": "npm run tsc -- -w",
    "plugin.sasswatch": "npm run scss && npm run scss -- --watch",
    "plugin.watch": "npm-run-all --parallel plugin.tscwatch plugin.sasswatch",
    "demo-ng.android.start": "cd ../demo-ng && tns debug android --syncAllFiles",
    "demo-ng.ios.start": "cd ../demo-ng && tns debug ios --syncAllFiles",
    "demo-ng.ios": "npm i && npm-run-all tsc --parallel plugin.watch demo-ng.ios.start",
    "demo-ng.android": "npm i && npm-run-all tsc --parallel plugin.watch demo-ng.android.start",
    "demo-ng.reset": "cd ../demo-ng && rimraf platforms",
    "plugin.prepare": "npm run build && (cd ../demo-ng && tns plugin remove @nota/nativescript-accessibility-ext && tns plugin add ../src)",
    "clean": "npm-run-all clean.demo-ng && npm run plugin.link",
    "clean.demo-ng": "cd ../demo-ng && rimraf hooks node_modules platforms package-lock.json",
    "clean.src": "cd ../src && rimraf node_modules package-lock.json",
    "ci.lint": "npm i && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Notalib/nativescript-accessibility-ext.git"
  },
  "keywords": [
    "NativeScript",
    "JavaScript",
    "Accessibility",
    "angular",
    "Android",
    "TalkBack",
    "VoiceOver",
    "iOS"
  ],
  "author": {
    "name": "Nota",
    "email": "app@nota.dk"
  },
  "contributors": [
    {
      "name": "Morten Anton Bach Sj√∏gren",
      "url": "http://mabs.dk",
      "email": "mabs@mabs.dk"
    },
    {
      "name": "Daniel Freiling",
      "email": "ddfreiling@gmail.com"
    }
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/Notalib/nativescript-accessibility-ext/issues"
  },
  "homepage": "https://github.com/Notalib/nativescript-accessibility-ext/#readme",
  "readmeFilename": "README.md",
  "devDependencies": {
    "nativescript-globalevents": "^1.2.2",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "prettier-check": "^2.0.0",
    "rimraf": "^2.6.3",
    "tns-core-modules": "~5.2.2",
    "tslint-no-circular-imports": "^0.6.2",
    "tns-platform-declarations": "~5.2.2",
    "tslint": "^5.14.0",
    "typescript": "~3.1.6"
  },
  "peerDependencies": {
    "tns-core-modules": "^5.2.2",
    "nativescript-globalevents": "^1.2.2"
  },
  "dependencies": {},
  "bootstrapper": "nativescript-plugin-seed"
}
